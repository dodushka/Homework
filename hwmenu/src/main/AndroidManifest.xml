<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="git.hwmenu" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OneActivity"
            android:label="@string/title_activity_one">
        <intent-filter>
            <action android:name="android.intent.action.showOneActivity"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
        </activity>
        <activity
            android:name=".TwoActivity"
            android:label="@string/title_activity_two" >
            <intent-filter>
                <action android:name="android.intent.action.showOneActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".SixActivity"
            android:label="@string/title_activity_six" >
            <intent-filter>
            <action android:name="android.intent.action.showSixActivity"/>
            <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

    </application>

</manifest>
